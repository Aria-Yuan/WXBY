<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    tools:context=".MySetting">

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="152dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="16dp"
        android:text="@string/Title_setting"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button113"
        android:layout_width="25dp"
        android:layout_height="26dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/tongzhi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.991"
        app:layout_constraintStart_toEndOf="@+id/textView35"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="250dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="85dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/sym_def_app_icon"
        android:contentDescription="@string/UserPhoto" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="96dp"
        android:text="@string/UserName"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.087"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="163dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="12dp"
        android:text="@string/UserId"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView36" />

    <Button
        android:id="@+id/button114"
        android:layout_width="35dp"
        android:layout_height="36dp"
        android:layout_marginEnd="18dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="72dp"
        android:background="@android:drawable/ic_menu_edit"
        android:backgroundTint="@android:color/holo_blue_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView37"
        app:layout_constraintTop_toBottomOf="@+id/button113" />

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/linearLayout"
        android:layout_width="350dp"
        android:layout_height="362dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="160dp"
        android:orientation="vertical"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintHorizontal_bias="0.0"
        card_view:layout_constraintStart_toStartOf="parent"
        card_view:layout_constraintTop_toTopOf="parent">

        <android.support.v7.widget.CardView
            android:id="@+id/sexual_setting"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:cardCornerRadius="4dp"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="208dp">

            <TextView
                android:id="@+id/sexual_title"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_margin="4dp"
                android:drawableStart="@drawable/sexual"
                android:gravity="center"
                android:text="@string/Sexual"
                android:textAlignment="inherit"
                android:textSize="20sp" />


        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/location_setting"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:cardCornerRadius="4dp"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="208dp">

            <TextView
                android:id="@+id/location_title"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_margin="4dp"
                android:drawableStart="@drawable/location"
                android:gravity="center"
                android:text="@string/LocationSetting"
                android:textAlignment="inherit"
                android:textSize="20sp" />

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/my_consultation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:cardCornerRadius="4dp"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="208dp">

            <TextView
                android:id="@+id/consultation_title"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_margin="4dp"
                android:drawableStart="@drawable/consultation"
                android:gravity="center"
                android:text="@string/MyConsultation"
                android:textAlignment="inherit"
                android:textSize="20sp" />

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/my_collections"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:cardCornerRadius="4dp"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="208dp">

            <TextView
                android:id="@+id/collection_title"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_margin="4dp"
                android:drawableStart="@drawable/my_collections"
                android:gravity="center"
                android:text="@string/MyCollections"
                android:textAlignment="inherit"
                android:textSize="20sp" />
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/my_question"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:cardCornerRadius="4dp"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="208dp">

            <TextView
                android:id="@+id/question_title"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_margin="4dp"
                android:drawableStart="@drawable/my_question"
                android:gravity="center"
                android:text="@string/MyQuestions"
                android:textAlignment="inherit"
                android:textSize="20sp" />
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/concerned_lawyers"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:cardCornerRadius="4dp"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="208dp">

            <TextView
                android:id="@+id/concerned_lawyers_title"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_margin="4dp"
                android:drawableStart="@drawable/concerned_lawyers"
                android:gravity="center"
                android:text="@string/ConcernedLawyers"
                android:textAlignment="inherit"
                android:textSize="20sp" />
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/settings"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:cardCornerRadius="4dp"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="208dp">

            <TextView
                android:id="@+id/settings_title"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_margin="4dp"
                android:drawableStart="@drawable/settings"
                android:gravity="center"
                android:text="@string/Settings"
                android:textAlignment="inherit"
                android:textSize="20sp" />
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/feedbacks"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:cardCornerRadius="4dp"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="208dp">

            <TextView
                android:id="@+id/feedbacks_title"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_margin="4dp"
                android:drawableStart="@drawable/feedback"
                android:gravity="center"
                android:text="@string/FeedbacksTitle"
                android:textAlignment="inherit"
                android:textSize="20sp" />

        </android.support.v7.widget.CardView>
    </LinearLayout>

    <Button
        android:id="@+id/button115"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@drawable/wenan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button116"
        android:layout_width="45dp"
        android:layout_height="50dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/shoye"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button115"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button117"
        android:layout_width="40dp"
        android:layout_height="45dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/wode"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button115" />

    <!-- A CardView that contains a TextView -->

</android.support.constraint.ConstraintLayout>